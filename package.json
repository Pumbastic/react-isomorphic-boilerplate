{
  "name": "react-isomorphic-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --profile --colors",
    "start": "cross-env NODE_ENV=production node ./prod/server.bundle.js",
    "entry": "babel-node server",
    "watch:hot": "cross-env NODE_ENV=development nodemon --exec npm run entry",
    "watch:server": "cross-env NODE_ENV=none nodemon --exec npm run entry",
    "hot": "webpack-dev-server --config webpack.config.devServer.js --open",
    "debug": "node --inspect ./prod/server.bundle.js"
  },
  "author": "Adrian Kontny",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "ejs": "^2.5.8",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.1",
    "morgan": "^1.9.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "clean-webpack-plugin": "^0.1.19",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "nodemon": "^1.17.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-node-externals": "^1.7.2"
  }
}
